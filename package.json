{
  "name": "typed-install",
  "version": "1.1.1",
  "description": "Install packages and types in one fell swoop.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "xavdid/typed-install",
  "bin": {
    "typedi": "lib/cli.js"
  },
  "dependencies": {
    "chalk": "4.1.0",
    "commander": "2.20.0",
    "debug": "4.3.1",
    "got": "10.7.0",
    "lodash": "4.17.20",
    "mz": "2.7.0",
    "ora": "4.0.3",
    "pkg-dir": "4.2.0",
    "shelljs": "0.8.4",
    "update-notifier": "4.1.0"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/got": "^9.0.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.117",
    "@types/mock-fs": "^3",
    "@types/mz": "2.7.0",
    "@types/node": "12.12.5",
    "@types/shelljs": "0.7.6",
    "@types/update-notifier": "4.1.0",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-config-xavdid": "1.0.0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11",
    "eslint-plugin-promise": "^4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-standard": "^4",
    "jest": "^25.2.7",
    "mock-fs": "^4.10.0",
    "ts-jest": "^25.3.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "predeploy": "rm -rf lib && npm rum build",
    "deploy": "npx np",
    "build": "tsc",
    "release": "npx np",
    "validate": "npm run test && npm run lint"
  },
  "keywords": [
    "typescript",
    "types",
    "npm",
    "yarn",
    "installing"
  ],
  "author": "David Brownman <beamneocube@gmail.com> (https://davidbrownman.com)",
  "license": "ISC",
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
